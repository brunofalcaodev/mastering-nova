{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch-overrides": "cross-env process.env.section=overrides NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch-public": "cross-env process.env.section=public NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.3.11",
        "tailwindcss": "^2.2.19"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.3.4",
        "cross-env": "^7.0.3",
        "tailwindcss-hero-patterns": "^0.0.1"
    }
}
