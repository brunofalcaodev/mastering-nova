{
    "name": "brunofalcaodev/mastering-nova",
    "description": "Mastering Nova course",
    "keywords": [
        "brunofalcaodev",
        "eduka",
        "laravel"
    ],
    "homepage": "https://github.com/brunofalcaodev/mastering-nova",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "brunocfalcao/flame": "dev-master",
        "brunofalcaodev/eduka": "dev-master",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "brunocfalcao/blade-feather-icons": "^2.0",
        "davidhsianturi/blade-bootstrap-icons": "^1.1",
        "thomaswelton/laravel-gravatar": "^1.3"

    },
    "autoload": {
        "psr-4": {
            "MasteringNova\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasteringNova\\MasteringNovaServiceProvider"
            ]
        }
    }
}
